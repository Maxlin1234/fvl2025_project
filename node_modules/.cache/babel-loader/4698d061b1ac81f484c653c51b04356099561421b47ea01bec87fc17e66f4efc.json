ntent {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 50px;\n}\n\n.contact-info h3 {\n  font-size: 1.8rem;\n  margin-bottom: 20px;\n  color: #2c3e50;\n}\n\n.contact-info p {\n  font-size: 1.1rem;\n  margin-bottom: 30px;\n  color: #666;\n}\n\n.contact-details {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.contact-item {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  font-size: 1.1rem;\n}\n\n.contact-icon {\n  font-size: 1.5rem;\n}\n\n.contact-form {\n  background: white;\n  padding: 40px;\n  border-radius: 15px;\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\n.form-group input,\n.form-group textarea {\n  width: 100%;\n  padding: 15px;\n  border: 2px solid #e1e5e9;\n  border-radius: 10px;\n  font-size: 1rem;\n  transition: border-color 0.3s ease;\n}\n\n.form-group input:focus,\n.form-group textarea:focus {\n  outline: none;\n  border-color: #42b983;\n}\n\n.submit-button {\n  width: 100%;\n  padding: 15px;\n  background: #42b983;\n  color: white;\n  border: none;\n  border-radius: 10px;\n  font-size: 1.1rem;\n  cursor: pointer;\n  transition: background 0.3s ease;\n}\n\n.submit-button:hover {\n  background: #3aa876;\n}\n\n/* 頁腳 */\n.footer {\n  background: #2c3e50;\n  color: white;\n  text-align: center;\n  padding: 30px 0;\n}\n\n/* 響應式設計 */\n@media (max-width: 768px) {\n  .nav-toggle {\n    display: flex;\n    align-items: center;\n    gap: 10px;\n    padding: 10px 15px;\n    background: #42b983;\n    color: white;\n    border-radius: 25px;\n    font-weight: 500;\n  }\n\n  .nav-panel {\n    width: 100%;\n    right: -100%;\n  }\n\n  .nav-panel-open {\n    right: 0;\n  }\n\n  .nav-panel-header {\n    padding: 20px 0;\n    border-bottom: 1px solid #eee;\n  }\n\n  .nav-section {\n    padding: 20px 0;\n    border-bottom: 1px solid #f0f0f0;\n  }\n\n  .nav-section:last-child {\n    border-bottom: none;\n  }\n\n  .nav-page-list li {\n    margin-bottom: 15px;\n  }\n\n  .nav-page-li