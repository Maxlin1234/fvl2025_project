ion-title {\n  text-align: center;\n  font-size: 2.5rem;\n  margin-bottom: 50px;\n  color: #2c3e50;\n}\n\n/* 關於區塊 */\n.about {\n  background: #f8f9fa;\n}\n\n.about-content {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 50px;\n  align-items: center;\n}\n\n.about-text p {\n  font-size: 1.1rem;\n  margin-bottom: 20px;\n  line-height: 1.8;\n}\n\n.about-stats {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  gap: 30px;\n}\n\n.stat-item {\n  text-align: center;\n  padding: 30px 20px;\n  background: white;\n  border-radius: 10px;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n}\n\n.stat-item h3 {\n  font-size: 2.5rem;\n  color: #42b983;\n  margin-bottom: 10px;\n}\n\n/* 服務區塊 */\n.services-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 30px;\n}\n\n.service-card {\n  background: white;\n  padding: 40px 30px;\n  border-radius: 15px;\n  text-align: center;\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s ease;\n}\n\n.service-card:hover {\n  transform: translateY(-10px);\n}\n\n.service-icon {\n  font-size: 3rem;\n  margin-bottom: 20px;\n}\n\n.service-card h3 {\n  font-size: 1.5rem;\n  margin-bottom: 15px;\n  color: #2c3e50;\n}\n\n.service-card p {\n  color: #666;\n  line-height: 1.6;\n}\n\n/* 作品集區塊 */\n.portfolio {\n  background: #f8f9fa;\n}\n\n.portfolio-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  gap: 30px;\n}\n\n.portfolio-item {\n  position: relative;\n  border-radius: 15px;\n  overflow: hidden;\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n}\n\n.portfolio-image img {\n  width: 100%;\n  height: 250px;\n  object-fit: cover;\n}\n\n.portfolio-overlay {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  background: linear-gradient(transparent, rgba(0, 0, 0, 0.8));\n  color: white;\n  padding: 30px 20px 20px;\n  transform: translateY(100%);\n  transition: transform 0.3s ease;\n}\n\n.portfolio-item:hover .portfolio-overlay {\n  transform: translateY(0);\n}\n\n.portfolio-overlay h3 {\n  margin-bottom: 10px;\n}\n\n/* 聯絡我們區塊 */\n.contact-content {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 50px;\n}\n\n.contact-info h3 {\n  color: #3ecfff;\n}\n\n.contact-info p {\n  font-size: 1.1rem;\n  margin-bottom: 30px;\n  color: #666;\n}\n\n.contact-details {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.contact-item {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  font-size: 1.1rem;\n}\n\n.contact-icon {\n  font-size: 1.5rem;\n}\n\n.contact-form {\n  background: white;\n  padding: 40px;\n  border-radius: 15px;\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\n.form-group input,\n.form-group textarea {\n  width: 100%;\n  padding: 15px;\n  border: 2px solid #e1e5e9;\n  border-radius: 10px;\n  font-size: 1rem;\n  transition: border-color 0.3s ease;\n}\n\n.form-group input:focus,\n.form-group textarea:focus {\n  outline: none;\n  border-color: #42b983;\n}\n\n.submit-button {\n  width: 100%;\n  padding: 15px;\n  background: #42b983;\n  color: white;\n  border: none;\n  border-radius: 10px;\n  font-size: 1.1rem;\n  cursor: pointer;\n  transition: background 0.3s ease;\n}\n\n.submit-button:hover {\n  background: #3aa876;\n}\n\n/* 頁腳 */\n.footer {\n  background: #2c3e50;\n  color: white;\n  text-align: center;\n  padding: 30px 0;\n}\n\n/* 響應式設計 */\n@media (max-width: 768px) {\n  .nav-toggle {\n    display: flex;\n    align-items: center;\n    gap: 10px;\n    padding: 10px 15px;\n    background: #42b983;\n    color: white;\n    border-radius: 25px;\n    font-weight: 500;\n  }\n\n  .nav-panel {\n    width: 100%;\n    right: -100%;\n  }\n\n  .nav-panel-open {\n    right: 0;\n  }\n\n  .nav-panel-header {\n    padding: 20px 0;\n    border-bottom: 1px solid #eee;\n  }\n\n  .nav-section {\n    padding: 20px 0;\n    border-bottom: 1px solid #f0f0f0;\n  }\n\n  .nav-section:last-child {\n    border-bottom: none;\n  }\n\n  .nav-page-list li {\n    margin-bottom: 15px;\n  }\n\n  .nav-page-list a {\n    display: block;\n    padding: 12px 0;\n    font-size: 1.1rem;\n  }\n\n  .hero {\n    flex-direction: column;\n    text-align: center;\n    padding: 100px 20px 50px;\n  }\n\n  .split{\n    font-size: 2.5rem;\n  }\n\n  .about-content,\n  .contact-content {\n    grid-template-columns: 1fr;\n    gap: 30px;\n  }\n\n  .about-stats {\n    grid-template-columns: 1fr;\n  }\n\n  .section-title {\n    font-size: 2rem;\n  }\n}\n\n@media (max-width: 480px) {\n  .hero-title {\n    font-size: 2rem;\n  }\n\n  .hero-subtitle {\n    font-size: 1.1rem;\n  }\n\n  .service-card,\n  .contact-form {\n    padding: 30px 20px;\n  }\n}\n\n/* 節目時間表區塊樣式 */\n.schedule {\n  background: #f8f9fa;\n  padding: 80px 0;\n}\n\n/* 日曆區塊樣式 */\n.calendar-section {\n  background: #fff;\n  padding: 80px 0;\n}\n\n.calendar-section .section-title {\n  color: #3ecfff;\n  margin-bottom: 40px;\n}\n.schedule-table-wrapper {\n  overflow-x: auto;\n}\n.schedule-table {\n  width: 100%;\n  border-collapse: collapse;\n  background: #fff;\n  border-radius: 12px;\n  box-shadow: 0 4px 24px rgba(0,0,0,0.06);\n  overflow: hidden;\n}\n.schedule-table th, .schedule-table td {\n  padding: 18px 20px;\n  text-align: left;\n}\n.schedule-table th {\n  background: #111;\n  color: #fff;\n  font-size: 1.1rem;\n  font-weight: 600;\n  border-bottom: 2px solid #222;\n}\n.schedule-table tr:not(:last-child) td {\n  border-bottom: 1px solid #eee;\n}\n.schedule-table tr:hover td {\n  background: #f0f0f0;\n}\n@media (max-width: 600px) {\n  .schedule-table th, .schedule-table td {\n    padding: 12px 8px;\n    font-size: 0.95rem;\n  }\n}\n\n/* 製作團隊區塊樣式 */\n.team {\n  background: #fff;\n  padding: 80px 0;\n}\n.team-list {\n  max-width: 700px;\n  margin: 0 auto;\n  display: flex;\n  flex-direction: column;\n  gap: 18px;\n  font-size: 1.13rem;\n}\n.team-item {\n  line-height: 1.7;\n  border-left: 4px solid #111;\n  padding-left: 16px;\n  background: #f8f9fa;\n  border-radius: 6px;\n  font-weight: 400;\n}\n.team-role {\n  font-weight: 700;\n  color: #111;\n  margin-right: 8px;\n}\n\n.contact .section-title {\n  color: #3ecfff;\n}\n\n.services .section-title {\n  color: #3ecfff;\n}\n\n/* GSAP 文字分割動畫樣式 */\n.char {\n  display: inline-block;\n  transform-origin: center;\n}\n\n.word {\n  display: inline-block;\n  margin-right: 0.2em;\n}\n\n/* 確保分割的文字在動畫期間不會換行 */\n.hero-title, .hero-subtitle, .split {\n  white-space: nowrap;\n  overflow: hidden;\n}\n\n/* .split 元素的特殊樣式 */\n.split {\n  font-size: 3.5rem;\n  font-weight: bold;\n  margin-bottom: 20px;\n  color: #fff;\n}\n\n@media (max-width: 768px) {\n  .hero-title, .hero-subtitle, .split {\n    white-space: normal;\n  }\n  .split {\n    font-size: 2.5rem;\n  }\n}\n\n.hero-banner-wrapper {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n  min-height: 500px;\n  overflow: hidden;\n}\n.hero-banner-img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  display: block;\n}\n.corner-btn {\n  position: absolute;\n  width: 100px;\n  height: 100px;\n  border-radius: 50%;\n  background: rgba(255,255,255,0.85);\n  border: 2px solid #333;\n  box-shadow: 0 2px 12px rgba(0,0,0,0.12);\n  z-index: 2;\n  transition: box-shadow 0.2s;\n  cursor: pointer;\n}\n.corner-btn:hover {\n  box-shadow: 0 4px 24px rgba(90,150,255,0.25);\n}\n.top-left { top: 24px; left: 24px; }\n.top-right { top: 24px; right: 24px; }\n.bottom-left { bottom: 24px; left: 24px; }\n.bottom-right { bottom: 24px; right: 24px; }\n</style>\n"],"mappings":";OAMaA,UAA6B;;EALnCC,EAAE,EAAC;AAAK;;EAGFA,EAAE,EAAC,MAAM;EAACC,KAAK,EAAC;;;EAClBA,KAAK,EAAC;AAAqB;;EAiBzBD,EAAE,EAAC,UAAU;EAACC,KAAK,EAAC;;;EACtBA,KAAK,EAAC;AAAW;;EAEfA,KAAK,EAAC;AAAwB;;EAC1BA,KAAK,EAAC;AAAgB;;EAqB1BD,EAAE,EAAC,UAAU;EAACC,KAAK,EAAC;;;EACtBA,KAAK,EAAC;AAAW;;EA+HfD,EAAE,EAAC,SAAS;EAACC,KAAK,EAAC;;;EACrBA,KAAK,EAAC;AAAW;;EAEfA,KAAK,EAAC;AAAiB;;EAoBnBA,KAAK,EAAC;AAAY;;EAGlBA,KAAK,EAAC;AAAY;;EAGlBA,KAAK,EAAC;AAAY;;;;uBA3MjCC,mBAAA,CAsNM,OAtNNC,UAsNM