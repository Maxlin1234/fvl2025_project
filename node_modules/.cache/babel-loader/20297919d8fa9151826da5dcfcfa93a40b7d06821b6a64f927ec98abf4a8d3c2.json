{"ast":null,"code":"import FooterBar from '../components/FooterBar.vue';\nimport Calendar from '../components/Calendar2.vue';\nimport Banner from '../components/Banner.vue';\nimport axios from 'axios';\nimport { gsap } from 'gsap';\nimport { SplitText } from 'gsap/SplitText';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\n// import * as THREE from 'three';\n\n// 這裡 import 10 張圖片\nimport img1 from '../assets/1.jpg';\nimport img2 from '../assets/2.jpg';\nimport img3 from '../assets/3.jpg';\n\n// 註冊 SplitText 和 ScrollTrigger 插件\ngsap.registerPlugin(SplitText, ScrollTrigger);\nexport default {\n  name: 'HomePage',\n  components: {\n    FooterBar,\n    Calendar,\n    Banner\n  },\n  data() {\n    return {\n      testdata: [],\n      firstImage: {},\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      schedule: [{\n        time: '10:00 - 11:00',\n        title: '開幕式',\n        location: '主舞台'\n      }, {\n        time: '11:15 - 12:00',\n        title: '主題演講：未來科技',\n        location: '會議廳A'\n      }, {\n        time: '13:00 - 14:30',\n        title: '工作坊：互動設計',\n        location: '教室B'\n      }, {\n        time: '15:00 - 16:00',\n        title: '圓桌論壇',\n        location: '會議廳A'\n      }, {\n        time: '16:30 - 17:00',\n        title: '閉幕式',\n        location: '主舞台'\n      }],\n      showLiveDrawer: false,\n      showDrawer: false,\n      showLectureDrawer: false,\n      showContentA: true,\n      showContentB: false,\n      currentEventType: 'exhibition',\n      // 當前選中的事件類型\n      portfolioList: []\n    };\n  },\n  computed: {\n    // 根據事件類型返回對應的照片\n    currentEventImage() {\n      const imageMap = {\n        'exhibition': img1,\n        // 開幕 - 顯示第一張照片\n        'workshop': img2,\n        // 播映 - 顯示第二張照片\n        'performance': img3 // LIVE演出 - 顯示第三張照片\n      };\n      return imageMap[this.currentEventType] || img1;\n    }\n  },\n  mounted() {\n    this.fetchData();\n    this.initCursor();\n    this.initTextAnimation();\n    this.initSunAnimation();\n    this.initCloud1Animation();\n    this.initCloud2Animation();\n    this.initServicesSliderScroll();\n  },\n  methods: {\n    async fetchData() {\n      try {\n        // 獲取第一張圖片\n        const response1 = await axios.get('https://unzip-clab-api.clab.org.tw/api/v1/schedules/5/works?page=0&limit=25', {\n          responseType: 'json'\n        });\n        this.testdata = response1.data.results[1].photo_1;\n        this.firstImage = this.testdata;\n        console.log(this.firstImage);\n\n        // 獲取作品集\n        const response2 = await axios.get('https://unzip-clab-api.clab.org.tw/api/v1/works?search=Future%20Vision%20Lab&limit=100', {\n          responseType: 'json'\n        });\n        this.portfolioList = response2.data;\n        console.log(this.portfolioList);\n      } catch (error) {\n        console.error('獲取數據失敗:', error);\n      }\n    },\n    initCursor() {\n      let cursor = document.querySelector('.cursor');\n      document.addEventListener('mousemove', e => {\n        cursor.style.left = e.pageX + 'px';\n        cursor.style.top = e.pageY + 'px';\n      });\n    },\n    toggleContent() {\n      this.showContentA = true;\n      this.showContentB = false;\n    },\n    toggleContent2() {\n      this.showContentB = true;\n      this.showContentA = false;\n    },\n    scrollToSection(sectionId) {\n      const element = document.getElementById(sectionId);\n      if (element) {\n        element.scrollIntoView({\n          behavior: 'smooth'\n        });\n      }\n    },\n    submitForm() {\n      alert('感謝您的訊息！我們會盡快回覆您。');\n      this.form = {\n        name: '',\n        email: '',\n        message: ''\n      };\n    },\n    initTextAnimation() {\n      // 分割 FUTURE VISION LAB 2025 標題\n      const splitTitle = document.querySelector('.split');\n      if (splitTitle) {\n        const splitText = new SplitText(splitTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        });\n\n        // 設置初始狀態\n        gsap.set(splitText.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        });\n\n        // 動畫效果\n        gsap.to(splitText.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        });\n      }\n\n      // 分割首頁標題文字\n      const heroTitle = document.querySelector('.hero-title');\n      if (heroTitle) {\n        const splitTitle = new SplitText(heroTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        });\n\n        // 設置初始狀態\n        gsap.set(splitTitle.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        });\n\n        // 動畫效果\n        gsap.to(splitTitle.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        });\n      }\n\n      // 分割副標題\n      const heroSubtitle = document.querySelector('.hero-subtitle');\n      if (heroSubtitle) {\n        const splitSubtitle = new SplitText(heroSubtitle, {\n          type: \"words\",\n          wordsClass: \"word\"\n        });\n        gsap.set(splitSubtitle.words, {\n          opacity: 0,\n          y: 30\n        });\n        gsap.to(splitSubtitle.words, {\n          opacity: 1,\n          y: 0,\n          duration: 0.6,\n          stagger: 0.1,\n          delay: 0.5,\n          ease: \"power2.out\"\n        });\n      }\n\n      // ScrollTrigger 動畫 - 節目時間表\n      gsap.fromTo('#schedule .section-title, #schedule .schedule-table', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#schedule\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 關於DOME\n      gsap.fromTo('#services .section-title, #services .service-card', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#services\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 作品集\n      gsap.fromTo('#portfolio .section-title, #portfolio .portfolio-item', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#portfolio\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 製作團隊\n      gsap.fromTo('#team .section-title, #team .team-item', {\n        opacity: 0,\n        y: 30\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.6,\n        stagger: 0.1,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#team\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 聯絡我們\n      gsap.fromTo('#contact .section-title, #contact .contact-info, #contact .contact-form', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.3,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#contact\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n    },\n    initSunAnimation() {\n      // 由於沒有太陽圖片元素，暫時註釋掉\n      // gsap.set(this.$refs.sunImg, {\n      //   y: '100%',\n      //   opacity: 1\n      // });\n\n      // gsap.to(this.$refs.sunImg, {\n      //   y: '0%',\n      //   repeat:0,\n      //   yoyo:true,\n      //   opacity: 1,\n      //   duration:2,\n      //   ease: \"power2.Out\",\n      // });\n    },\n    initCloud1Animation() {\n      gsap.set(this.$refs.cloud1Img, {\n        y: '100%',\n        opacity: 1\n      });\n\n      // 創建雲移動的動畫\n      gsap.to(this.$refs.cloud1Img, {\n        y: '0%',\n        opacity: 1,\n        duration: 4,\n        ease: \"power2.out\"\n      });\n    },\n    initCloud2Animation() {\n      gsap.set(this.$refs.cloud2Img, {\n        y: '100%',\n        opacity: 1\n      });\n\n      // 創建雲移動的動畫\n      gsap.to(this.$refs.cloud2Img, {\n        y: '0%',\n        opacity: 1,\n        duration: 3,\n        ease: \"power2.out\"\n      });\n    },\n    scrollServices(direction) {\n      const slider = this.$refs.servicesSlider;\n      const card = slider.querySelector('.service-card');\n      if (slider && card) {\n        const scrollAmount = card.offsetWidth + 24; // 24px gap\n        slider.scrollBy({\n          left: direction * scrollAmount,\n          behavior: 'smooth'\n        });\n      }\n    },\n    initServicesSliderScroll() {\n      this.$nextTick(() => {\n        const slider = this.$refs.servicesSlider;\n        if (!slider) return;\n        const container = slider.parentElement;\n        const cardWidth = 320; // 卡片寬度\n        const gap = 24; // 間距\n        const containerPadding = 40; // 容器的左右padding總和\n\n        // 計算容器實際可用寬度\n        const availableWidth = container.offsetWidth - containerPadding;\n        const cardsPerView = Math.floor(availableWidth / (cardWidth + gap)); // 可視卡片數量\n        const totalCards = slider.children.length;\n\n        // 計算最大可滑動距離，確保最後一張卡片完整顯示\n        const maxScroll = Math.max(0, (totalCards - cardsPerView) * (cardWidth + gap));\n        console.log('Container width:', container.offsetWidth);\n        console.log('Available width:', availableWidth);\n        console.log('Cards per view:', cardsPerView);\n        console.log('Total cards:', totalCards);\n        console.log('Max scroll:', maxScroll);\n        if (maxScroll <= 0) return;\n        gsap.set(slider, {\n          x: 0\n        });\n        gsap.to(slider, {\n          x: -maxScroll,\n          // 內容往左滑\n          ease: 'none',\n          scrollTrigger: {\n            trigger: '#services',\n            start: 'top top',\n            end: '+=' + maxScroll,\n            scrub: 1.2,\n            pin: true,\n            anticipatePin: 1,\n            markers: true\n          }\n        });\n      });\n    },\n    handleEventTypeChange(eventType) {\n      this.currentEventType = eventType;\n    }\n  }\n};","map":{"version":3,"names":["FooterBar","Calendar","Banner","axios","gsap","SplitText","ScrollTrigger","img1","img2","img3","registerPlugin","name","components","data","testdata","firstImage","form","email","message","schedule","time","title","location","showLiveDrawer","showDrawer","showLectureDrawer","showContentA","showContentB","currentEventType","portfolioList","computed","currentEventImage","imageMap","mounted","fetchData","initCursor","initTextAnimation","initSunAnimation","initCloud1Animation","initCloud2Animation","initServicesSliderScroll","methods","response1","get","responseType","results","photo_1","console","log","response2","error","cursor","document","querySelector","addEventListener","e","style","left","pageX","top","pageY","toggleContent","toggleContent2","scrollToSection","sectionId","element","getElementById","scrollIntoView","behavior","submitForm","alert","splitTitle","splitText","type","charsClass","wordsClass","set","chars","opacity","y","rotationX","to","duration","stagger","ease","heroTitle","heroSubtitle","splitSubtitle","words","delay","fromTo","scrollTrigger","trigger","start","end","toggleActions","$refs","cloud1Img","cloud2Img","scrollServices","direction","slider","servicesSlider","card","scrollAmount","offsetWidth","scrollBy","$nextTick","container","parentElement","cardWidth","gap","containerPadding","availableWidth","cardsPerView","Math","floor","totalCards","children","length","maxScroll","max","x","scrub","pin","anticipatePin","markers","handleEventTypeChange","eventType"],"sources":["/Users/linhankuan/fvl2025/src/views/HomePage.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 首頁區塊 -->\n    <section id=\"home\" class=\"hero\" style=\"position: relative;\">\n      <div class=\"hero-banner-wrapper\" style=\"position: absolute;bottom: 0;filter: brightness(105%);\">\n        <div class=\"cursor\"></div>\n    \n         <h2 class=\"banner-title\">2025/11/01~2025/12/22</h2>\n     \n       <Banner/>\n       <button class=\"corner-btn top-left\" \n               @mouseenter=\"showDrawer = true\">\n         <span class=\"btn-zh\">場次表</span>\n         <span class=\"btn-en\">TIME</span>\n       </button>\n       <button class=\"corner-btn  bottom-right\" \n               @mouseenter=\"showLiveDrawer = true\">\n         <span class=\"btn-zh\">關於我們</span>\n         <span class=\"btn-en\">ABOUT</span>\n       </button>\n       <button class=\"corner-btn bottom-left\" \n               @mouseenter=\"showLectureDrawer = true; showDrawer = false; showLiveDrawer = false;\">\n         <span class=\"btn-zh\">入場須知</span>\n         <span class=\"btn-en\">INFO</span>\n       </button>\n      </div>\n    </section>\n\n    <!-- 節目時間表區塊 -->\n     <section id=\"schedule\" class=\"schedule\">\n      <div class=\"container\">\n        <h2 class=\"section-title\" style=\"color: lightgray;\">節目時間表</h2>\n        <div class=\"schedule-table-wrapper\">\n          <table class=\"schedule-table\">\n            <thead>\n              <tr>\n                <th>時間</th>\n                <th>節目名稱</th>\n                <th>地點</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"item in schedule\" :key=\"item.time + item.title\">\n                <td>{{ item.time }}</td>\n                <td>{{ item.title }}</td>\n                <td>{{ item.location }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </section> \n\n    <!-- 2025年11月日曆區塊 -->\n    <section id=\"calendar\" class=\"calendar-section\">\n      <div class=\"container\">\n        <div class=\"wrap1\" style=\" margin:0;padding:0;font-family: 'Poppins', sans-serif;\">\n          <div class=\"banner2\">\n            <video autoplay muted loop style=\"width: 100%;\" > \n              <source src=\"../assets/dome.mp4\" type=\"video/mp4\">\n            </video>\n              <h2 style=\"font-size:5em !important;\">活動日曆</h2>\n          </div>\n        </div>\n\n         <ul class=\"list-show_content\" style=\"display: flex;flex-direction: column;color: black;padding: 0;\">\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.11.19(三)</div>\n            <div class=\"title\">貴賓專場V1</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.11.19(四)</div>\n            <div class=\"title\">貴賓專場V2</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.11.21(五)</div>\n            <div class=\"title\">2025 FUTURE VISION 開幕</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.11.22(六)</div>\n            <div class=\"title\">播映</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.11.23(日)</div>\n            <div class=\"title\">播映</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.11.28(五)</div>\n            <div class=\"title\">三牲獻藝(tbd)</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.11.29(六)</div>\n            <div class=\"title\">三牲獻藝(tbd)</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.11.30(日)</div>\n            <div class=\"title\">播映</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.12.6(六)</div>\n            <div class=\"title\">安娜祺</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.12.7(日)</div>\n            <div class=\"title\">安娜祺</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.12.12(五)</div>\n            <div class=\"title\">學校合作</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\">\n            <div class=\"date\">2025.12.13(六)</div>\n            <div class=\"title\">學校合作</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n\n          <li class=\"list-show_content_item\" style=\"background-color: aliceblue;\">\n            <div class=\"date\">2025.12.14(日)</div>\n            <div class=\"title\">播映</div>\n            <div class=\"time\">12:00AM~13:00PM</div>\n          </li>\n         </ul>\n      </div>\n    </section>\n\n    <section id=\"fix-section\" style=\" height: calc(100vh + 20px);width: 100vw;background-color:#fff;position: relative;\">\n       <div class=\"title-box\" style=\"position:absolute;bottom: 0;left: 50%;width:50vw;background-color:#fff;z-index: 2;padding: 40px;-webkit-transform:translateX(-50%)\">\n         <h2 style=\"color:black;\">FUTURE VISION LAB 實驗展演計劃，穹頂劇場 DOME 怎麼創作怎麼看？──專訪蔡奇宏 ╳ 吳秉聖</h2>\n       </div>\n       <div class=\"fix-bg\"></div>\n    </section>\n\n    <!-- 關於區塊 -->\n    <section id=\"about\" class=\"about\">\n      <div class=\"container\" style=\"background-color: #fff;padding: 30px 30px 10px;\">\n        <div class=\"about-content\">\n          <div class=\"about-text\">\n            <h2>未來視覺實驗室：一顆球的誕生</h2>\n            <p>DOME 的開始是這樣的。</p>\n            <p>隨著空軍總部遷址大直，建國南路與仁愛路交會處這塊日治時代便開發、如今左帝寶右吾疆的精華地點，2018 年由文化部接手改造成文化實驗基地啟動臺灣當代文化實驗場（C-LAB）。而在 2020 年身為 C-LAB 一員的科技媒體實驗平台響應「卸下圍牆，打開空總」政策，未來視覺實驗室也在此時成立，同時思考科技媒體實驗該往哪些方向發展，那目光兜兜繞繞，落在彼時盛行的 VR 科技藝術，而以「裸眼 VR」為目標的 FUTURE VISION LAB 計劃因應啟動。</p>\n            <p>想到 VR 技術，浮上眼前是每個人各自套上頭盔設備，漂浮於個體的世界。但是對 C-LAB 科技媒體實驗平台研發企劃組蔡奇宏主任來說，如果可以搭造一個空間，讓大家裸眼就能看到 VR 效果，創造集體的共享就太好了。「五十個人在同一空間裡面觀看，不需依賴設備，卻又可同時感受 VR 帶來的立體視覺效果或聲音，感官體驗是不一樣的。」\n            蔡奇宏解釋，傳統電影平面且鏡框的形式，多少和觀眾拉出一段距離，然而球型空間的包覆性則讓人們對於邊界的感知消失，特別是當人們躺臥在地板時，失去當下對於空間的感受或是平衡感，容易產生落入影像的錯覺。</p>\n          </div>\n          <div class=\"about-stats\">\n            <img :src=\"'https://unzip-clab-api.clab.org.tw/'+ firstImage\" style=\"width: 100%;\">\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <div class=\"wrap\" style=\" margin:0;padding:0;font-family: 'Poppins', sans-serif;\">\n      <div class=\"banner\">\n        <video autoplay muted loop style=\"width: 100%;\" > \n          <source src=\"../assets/dome.mp4\" type=\"video/mp4\">\n        </video>\n        <h2>DOME</h2>\n      </div>\n    </div>\n\n    <!-- 作品集區塊 -->\n    <section id=\"portfolio\" class=\"portfolio\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">播映作品</h2>\n        <div class=\"portfolio-grid\">\n          <div class=\"portfolio-item\" v-for=\"(item, idx) in portfolioList\" :key=\"idx\">\n            <router-link :to=\"{name:'PortfolioDetil',}\">\n            <div class=\"portfolio-image\" style=\"height: 100%;\">\n              <img :src=\"'https://unzip-clab-api.clab.org.tw/' + item.photo_1\"  :alt=\"item.title\" style=\"width: 100%;height: 100%;\">\n            </div>\n            <div class=\"portfolio-overlay\">\n              <h3>{{ item.work_zh.title }}</h3>\n              <p>{{ item.work_en.title }}</p>\n            </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 製作團隊區塊 -->\n    <section id=\"team\" class=\"team\" style=\"padding: 0;margin: 0;overflow: hidden;position: relative;\">\n      <div class=\"container\" style=\"width: 100vw;height: 105vh;padding: 0;\">\n        <div class=\"team-content\">\n          <h2 class=\"section-title\" style=\"margin: 0;padding: 1rem;\">製作團隊</h2>\n          <div class=\"team-list\">\n            <div class=\"team-item\"><span class=\"team-role\">計畫主持人</span>：蔡奇宏</div>\n            <div class=\"team-item\"><span class=\"team-role\">節目統籌</span>：廖苑喻</div>\n            <div class=\"team-item\"><span class=\"team-role\">技術統籌</span>：蔡奇宏</div>\n            <div class=\"team-item\"><span class=\"team-role\">技術執行</span>：王竣恆</div>\n            <div class=\"team-item\"><span class=\"team-role\">技術協力</span>：林瀚寬</div>\n            <div class=\"team-item\"><span class=\"team-role\">前期建築概念設計</span>：陽明交通大學建築研究所 JHStudio</div>\n            <div class=\"team-item\"><span class=\"team-role\">建築協力</span>：achy_made</div>\n            <div class=\"team-item\"><span class=\"team-role\">聲場設計</span>：C-LAB 臺灣聲響實驗室</div>\n            <div class=\"team-item\"><span class=\"team-role\">音響系統統籌</span>：黑米創意工作室</div>\n            <div class=\"team-item\"><span class=\"team-role\">開場影像</span>：吳克軍</div>\n            <div class=\"team-item\"><span class=\"team-role\">進場影像</span>：榊原澄人</div>\n            <div class=\"team-item\"><span class=\"team-role\">主視覺設計</span>：林羅伯</div>\n            <div class=\"team-item\"><span class=\"team-role\">網站設計</span>：林瀚寬</div>\n            <div class=\"team-item\"><span class=\"team-role\">靜態攝影</span>：林軒朗、ANPIS FOTO 王世邦</div>\n            <div class=\"team-item\"><span class=\"team-role\">動態攝影</span>：散步映畫有限公司</div>\n            <div class=\"team-item\"><span class=\"team-role\">宣傳片剪接</span>：黃嘉文</div>\n            <div class=\"team-item\"><span class=\"team-role\">翻譯</span>：高慧倩</div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 頁腳 -->\n    <FooterBar />\n\n    <!-- LiveDrawer 結構 -->\n    <div class=\"live-drawer\" :class=\"{ open: showLiveDrawer }\">\n      <button class=\"drawer-close\" @click=\"showLiveDrawer = false\">×</button>\n      <div class=\"drawer-content\">\n        <img :src=\"currentEventImage\" style=\"width:100%;height:100%;z-index:999;object-fit:cover;\">\n        <div class=\"drawer-title\" >\n        <h2 style=\"margin: 0;padding:10px;\">2025精選</h2>\n        <p style=\"padding:10px;\">分享C-LAB未來視覺實驗室自2020年起持續推動實驗展演計畫「FUTURE VISION LAB」，以數位實驗建築著手打造穹形場域（DOME），以探索科技媒體的視覺極限為目標，過去四年已進行百餘件作品展演。此計畫在2023年完成軟硬體升級，尺寸突破過往尺度，打造直徑15公尺、全臺唯一巨型移動式戶外沉浸式體驗空間「C-LAB穹頂劇場」。影音軟硬體歷經數次迭代開發，克服球形曲面投影的校正、融接、對位、播放控制與影像前製等多重技術之複雜度，沉浸影像投影系統總運算可達8K*8K超高解析度；內部以雙層結構隱藏影音設備，客製新型態透聲投影膜片，並與臺灣聲響實驗室攜手合作建構更完善的25.4聲道環繞聲場環境。</p>\n        </div>\n      </div>\n    </div>\n\n    <!-- Drawer 結構 -->\n    <div class=\"drawer\" :class=\"{ open: showDrawer }\">\n      <button class=\"drawer-close\" @click=\"showDrawer = false\">×</button>\n      <div class=\"drawer-content2\">\n        <div class=\"cal-page\">\n          <div class=\"cal-title\">\n              <h2 style=\"padding: 20px;color: #fff;\">FUTURE VISION LAB <br>2025 節目表</h2>\n              <Calendar @event-type-changed=\"handleEventTypeChange\" />\n          </div>\n          <img :src=\"currentEventImage\" alt=\"event image\">\n        </div>\n      </div>\n    </div>\n\n    <!-- 講座 Drawer 結構 -->\n    <div class=\"lecture-drawer\" :class=\"{ open: showLectureDrawer }\">\n      <button class=\"drawer-close\" @click=\"showLectureDrawer = false\">×</button>\n      <div class=\"logo-contain\" style=\"width: 100vw;height: auto;display:flex;justify-content: center;\">\n                <img src=\"../assets/fvl_logo.png\" style=\"width:120px;position: absolute;z-index: 999;left: 0;top:5%;\">\n       </div>\n\n      <div class=\"drawer-content\" style=\"background-color:black;color:white;\">\n                  <video autoplay muted loop src=\"../assets/dome.mp4\" style=\"position: absolute;z-index: 1;right:0%;width:100%;height:70%;top:-5%; float: left;\n       clip-path: ellipse(50% 50% at right);opacity: 0.5;object-fit:cover;\"></video>\n        <div class=\"all-content\" style=\"position: absolute;z-index: 999;height: 100%;width: 100%; flex-direction: column;display: flex;justify-content: center;align-items:flex-start;\">\n        <h2 style=\"color:gainsboro;border-bottom: 2px solid white;\">觀眾入場須知</h2>\n        <div class=\"list-text\" >\n          <button class=\"list-btn1\" @click=\"toggleContent()\">關於入場規章</button>\n        <button class=\"list-btn2\"  @click=\"toggleContent2()\">攝錄影需知</button>\n        </div>\n        <div class=\"content-container\">\n        <div class=\"lecture-list\" v-if=\"showContentA\" style=\"font-size: 15px;\">\n          <p>參展廠商、參觀者或展館區域內人員，有下列情形且經主辦單位勸導未改善者，主辦單位得拒絕其進場或要求違反者離場，並禁止再行進入展場，不予退費：</p>\n          <p> 1.違反公共秩序或善良風俗，或影響展場形象、衛生安全，或其他影響展覽秩序之行為，包含但不限於下列之行為：</p>\n          <p>(1)騷擾、大聲咆哮、辱罵、恐嚇或傷害展場工作人員、其他相關人員。</p>\n          <p>(2)於展場使用不適當移動工具，如兩輪平衡車、滑板等。</p>\n          <p>(3)於展場酒醉失態。</p>\n          <p>(4)攜帶違禁物、爆裂物、危險物品、其物品有致人、財物等損害之虞或可能影響展場秩序、安全之物品。</p>\n          <p>(5)穿著惡臭衣服、拖鞋、攜帶不潔物品等影響展場形象。</p>\n          <p>(6)其他影響展覽進行之行為。</p>\n          <p>2.於展館從事之抗議、陳情等活動，影響展場秩序及觀展品質。</p>\n          <p>3.參展商或其他出入展場之人員拒絕依主辦單位規定之方式入場。</p>\n          <p>4.出入展場相關人員使用他人身分證明文件或變造、偽造身分證明文件。</p>\n          <p>5.展覽期間5月20至5月23日，禁止18歲以下者入場，以維持展場秩序及安全</p>\n        </div>\n        <div class=\"lecture-list\" v-if=\"showContentB\" style=\"font-size: 15px;\">\n          <p>1.館內不開放商業攝影（例：婚紗、商品型錄、攝影教學等）或其他影響場館安全及參觀品質之拍攝。</p>\n          <p>2.館內除另有特別告示，開放非商業性攝影。為維護及尊重創作者智慧財產權， 請勿翻攝展場內錄像作品及影片。</p>\n          <p>3.攝影請勿使用閃光燈、三腳架、手持穩定器或自拍棒等各式專業器材或輔助設備。</p>\n        </div>\n        </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport FooterBar from '../components/FooterBar.vue';\nimport Calendar from '../components/Calendar2.vue';\nimport Banner from '../components/Banner.vue';\nimport axios from 'axios';\nimport { gsap } from 'gsap';\nimport { SplitText } from 'gsap/SplitText';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\n// import * as THREE from 'three';\n\n// 這裡 import 10 張圖片\nimport img1 from '../assets/1.jpg';\nimport img2 from '../assets/2.jpg';\nimport img3 from '../assets/3.jpg';\n\n// 註冊 SplitText 和 ScrollTrigger 插件\ngsap.registerPlugin(SplitText, ScrollTrigger);\n\nexport default {\n  name: 'HomePage',\n  components: { FooterBar, Calendar, Banner },\n  data() {\n    return {\n      testdata:[],\n      firstImage:{},\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      schedule: [\n        { time: '10:00 - 11:00', title: '開幕式', location: '主舞台' },\n        { time: '11:15 - 12:00', title: '主題演講：未來科技', location: '會議廳A' },\n        { time: '13:00 - 14:30', title: '工作坊：互動設計', location: '教室B' },\n        { time: '15:00 - 16:00', title: '圓桌論壇', location: '會議廳A' },\n        { time: '16:30 - 17:00', title: '閉幕式', location: '主舞台' }\n      ],\n      showLiveDrawer: false,\n      showDrawer: false,\n      showLectureDrawer: false,\n      showContentA: true,\n      showContentB: false,\n      currentEventType: 'exhibition', // 當前選中的事件類型\n      portfolioList: [],\n    }\n  },\n  computed: {\n    // 根據事件類型返回對應的照片\n    currentEventImage() {\n      const imageMap = {\n        'exhibition': img1, // 開幕 - 顯示第一張照片\n        'workshop': img2,   // 播映 - 顯示第二張照片\n        'performance': img3 // LIVE演出 - 顯示第三張照片\n      };\n      return imageMap[this.currentEventType] || img1;\n    }\n  },\n  mounted() {\n    this.fetchData();\n    this.initCursor();\n    this.initTextAnimation();\n    this.initSunAnimation();\n    this.initCloud1Animation();\n    this.initCloud2Animation();\n    this.initServicesSliderScroll();\n  },\n  methods: {\n    async fetchData() {\n      try {\n        // 獲取第一張圖片\n        const response1 = await axios.get('https://unzip-clab-api.clab.org.tw/api/v1/schedules/5/works?page=0&limit=25', {responseType: 'json'});\n        this.testdata = response1.data.results[1].photo_1;\n        this.firstImage = this.testdata;\n        console.log(this.firstImage);\n\n        // 獲取作品集\n        const response2 = await axios.get('https://unzip-clab-api.clab.org.tw/api/v1/works?search=Future%20Vision%20Lab&limit=100', {responseType: 'json'});\n        this.portfolioList = response2.data;\n        console.log(this.portfolioList);\n      } catch (error) {\n        console.error('獲取數據失敗:', error);\n      }\n    },\n\n    initCursor() {\n      let cursor = document.querySelector('.cursor');\n      document.addEventListener('mousemove',(e)=>{\n        cursor.style.left = e.pageX + 'px';\n        cursor.style.top = e.pageY + 'px';\n      })\n    },\n\n    toggleContent() {\n      this.showContentA = true;\n      this.showContentB = false;\n    },\n\n    toggleContent2() {\n      this.showContentB = true;\n      this.showContentA = false;\n    },\n\n    scrollToSection(sectionId) {\n      const element = document.getElementById(sectionId);\n      if (element) {\n        element.scrollIntoView({ behavior: 'smooth' });\n      }\n    },\n\n    submitForm() {\n      alert('感謝您的訊息！我們會盡快回覆您。');\n      this.form = { name: '', email: '', message: '' };\n    },\n\n    initTextAnimation() {\n      // 分割 FUTURE VISION LAB 2025 標題\n      const splitTitle = document.querySelector('.split')\n      if (splitTitle) {\n        const splitText = new SplitText(splitTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        })\n        \n        // 設置初始狀態\n        gsap.set(splitText.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        })\n        \n        // 動畫效果\n        gsap.to(splitText.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        })\n      }\n      \n      // 分割首頁標題文字\n      const heroTitle = document.querySelector('.hero-title')\n      if (heroTitle) {\n        const splitTitle = new SplitText(heroTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        })\n        \n        // 設置初始狀態\n        gsap.set(splitTitle.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        })\n        \n        // 動畫效果\n        gsap.to(splitTitle.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        })\n      }\n      \n      // 分割副標題\n      const heroSubtitle = document.querySelector('.hero-subtitle')\n      if (heroSubtitle) {\n        const splitSubtitle = new SplitText(heroSubtitle, {\n          type: \"words\",\n          wordsClass: \"word\"\n        })\n        \n        gsap.set(splitSubtitle.words, {\n          opacity: 0,\n          y: 30\n        })\n        \n        gsap.to(splitSubtitle.words, {\n          opacity: 1,\n          y: 0,\n          duration: 0.6,\n          stagger: 0.1,\n          delay: 0.5,\n          ease: \"power2.out\"\n        })\n      }\n      \n      // ScrollTrigger 動畫 - 節目時間表\n      gsap.fromTo('#schedule .section-title, #schedule .schedule-table', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#schedule\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n            toggleActions: \"play none none reverse\"\n          }\n        }\n      )\n      \n      // ScrollTrigger 動畫 - 關於DOME\n      gsap.fromTo('#services .section-title, #services .service-card', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#services\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n            toggleActions: \"play none none reverse\"\n          }\n        }\n      )\n      \n      // ScrollTrigger 動畫 - 作品集\n      gsap.fromTo('#portfolio .section-title, #portfolio .portfolio-item', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#portfolio\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n            toggleActions: \"play none none reverse\"\n          }\n        }\n      )\n      \n      // ScrollTrigger 動畫 - 製作團隊\n      gsap.fromTo('#team .section-title, #team .team-item', \n        {\n          opacity: 0,\n          y: 30\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.6,\n          stagger: 0.1,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#team\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n 