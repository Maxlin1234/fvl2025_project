{"ast":null,"code":"// import NavBar from './components/NavBar.vue';\nimport FooterBar from './components/FooterBar.vue';\nimport Calendar from './components/Calendar.vue';\nimport axios from 'axios';\nimport { gsap } from 'gsap';\nimport { SplitText } from 'gsap/SplitText';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\n\n// 註冊 SplitText 和 ScrollTrigger 插件\ngsap.registerPlugin(SplitText, ScrollTrigger);\nexport default {\n  name: 'App',\n  components: {\n    FooterBar,\n    Calendar\n  },\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      schedule: [{\n        time: '10:00 - 11:00',\n        title: '開幕式',\n        location: '主舞台'\n      }, {\n        time: '11:15 - 12:00',\n        title: '主題演講：未來科技',\n        location: '會議廳A'\n      }, {\n        time: '13:00 - 14:30',\n        title: '工作坊：互動設計',\n        location: '教室B'\n      }, {\n        time: '15:00 - 16:00',\n        title: '圓桌論壇',\n        location: '會議廳A'\n      }, {\n        time: '16:30 - 17:00',\n        title: '閉幕式',\n        location: '主舞台'\n      }],\n      showLiveDrawer: false,\n      showDrawer: false\n    };\n  },\n  mounted() {\n    this.initTextAnimation();\n    this.fetchDatabaseData();\n    this.pinBanner();\n  },\n  methods: {\n    // 取得資料庫資料並在 console 顯示\n    async fetchDatabaseData() {\n      try {\n        const response = await axios.get('http://localhost:3001/api/data');\n        console.log(response.data);\n        // console.log('📋 資料筆數:', response.data.length);\n        // console.log('🔍 詳細資料:', JSON.stringify(response.data, null, 2));\n        return response.data;\n      } catch (error) {\n        console.error('❌ 取得資料庫資料失敗:', error);\n        return [];\n      }\n    },\n    scrollToSection(sectionId) {\n      const element = document.getElementById(sectionId);\n      if (element) {\n        element.scrollIntoView({\n          behavior: 'smooth'\n        });\n      }\n    },\n    submitForm() {\n      alert('感謝您的訊息！我們會盡快回覆您。');\n      this.form = {\n        name: '',\n        email: '',\n        message: ''\n      };\n    },\n    initTextAnimation() {\n      // 分割 FUTURE VISION LAB 2025 標題\n      const splitTitle = document.querySelector('.split');\n      if (splitTitle) {\n        const splitText = new SplitText(splitTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        });\n\n        // 設置初始狀態\n        gsap.set(splitText.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        });\n\n        // 動畫效果\n        gsap.to(splitText.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        });\n      }\n\n      // 分割首頁標題文字\n      const heroTitle = document.querySelector('.hero-title');\n      if (heroTitle) {\n        const splitTitle = new SplitText(heroTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        });\n\n        // 設置初始狀態\n        gsap.set(splitTitle.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        });\n\n        // 動畫效果\n        gsap.to(splitTitle.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        });\n      }\n\n      // 分割副標題\n      const heroSubtitle = document.querySelector('.hero-subtitle');\n      if (heroSubtitle) {\n        const splitSubtitle = new SplitText(heroSubtitle, {\n          type: \"words\",\n          wordsClass: \"word\"\n        });\n        gsap.set(splitSubtitle.words, {\n          opacity: 0,\n          y: 30\n        });\n        gsap.to(splitSubtitle.words, {\n          opacity: 1,\n          y: 0,\n          duration: 0.6,\n          stagger: 0.1,\n          delay: 0.5,\n          ease: \"power2.out\"\n        });\n      }\n\n      // ScrollTrigger 動畫 - 節目時間表\n      gsap.fromTo('#schedule .section-title, #schedule .schedule-table', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#schedule\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 關於DOME\n      gsap.fromTo('#services .section-title, #services .service-card', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#services\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 作品集\n      gsap.fromTo('#portfolio .section-title, #portfolio .portfolio-item', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.2,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#portfolio\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 製作團隊\n      gsap.fromTo('#team .section-title, #team .team-item', {\n        opacity: 0,\n        y: 30\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.6,\n        stagger: 0.1,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#team\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n\n      // ScrollTrigger 動畫 - 聯絡我們\n      gsap.fromTo('#contact .section-title, #contact .contact-info, #contact .contact-form', {\n        opacity: 0,\n        y: 50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 0.8,\n        stagger: 0.3,\n        ease: \"power2.out\",\n        scrollTrigger: {\n          trigger: \"#contact\",\n          start: \"top 80%\",\n          end: \"bottom 20%\",\n          toggleActions: \"play none none reverse\"\n        }\n      });\n    },\n    pinBanner() {\n      gsap.registerPlugin(ScrollTrigger);\n      gsap.timeline({\n        scrollTrigger: {\n          trigger: \".hero-banner-img\",\n          start: \"top top\",\n          end: \"+=1800\",\n          pin: \"#home\"\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["FooterBar","Calendar","axios","gsap","SplitText","ScrollTrigger","registerPlugin","name","components","data","form","email","message","schedule","time","title","location","showLiveDrawer","showDrawer","mounted","initTextAnimation","fetchDatabaseData","pinBanner","methods","response","get","console","log","error","scrollToSection","sectionId","element","document","getElementById","scrollIntoView","behavior","submitForm","alert","splitTitle","querySelector","splitText","type","charsClass","wordsClass","set","chars","opacity","y","rotationX","to","duration","stagger","ease","heroTitle","heroSubtitle","splitSubtitle","words","delay","fromTo","scrollTrigger","trigger","start","end","toggleActions","timeline","pin"],"sources":["/Users/linhankuan/fvl2025/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- <NavBar :scrollToSection=\"scrollToSection\" /> -->\n    <!-- 首頁區塊 -->\n    <section id=\"home\" class=\"hero\">\n      <div class=\"hero-banner-wrapper\">\n        <img src=\"./assets/2025banner.jpg\" alt=\"2025banner\" class=\"hero-banner-img\">\n        <button class=\"corner-btn top-left\"  @click=\"showDrawer= true\">播映作品</button>\n        <button class=\"corner-btn top-right\" @click=\"showLiveDrawer = true\">LIVE 演出</button>\n        <button class=\"corner-btn bottom-left\">講座</button>\n        <button class=\"corner-btn bottom-right\" @click=\"scrollToSection('services')\">瞭解更多</button>\n      </div>\n \n        <!-- <h1 class=\"split\">FUTURE VISION LAB 2025</h1> -->\n\n        <!-- <p class=\"hero-subtitle\">DOME球形展演空間</p> -->\n        <!-- <button class=\"cta-button\" @click=\"scrollToSection('services')\">了解更多</button> -->\n\n\n    </section>\n\n    <!-- 節目時間表區塊 -->\n    <!-- <section id=\"schedule\" class=\"schedule\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">節目時間表</h2>\n        <div class=\"schedule-table-wrapper\">\n          <table class=\"schedule-table\">\n            <thead>\n              <tr>\n                <th>時間</th>\n                <th>節目名稱</th>\n                <th>地點</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"item in schedule\" :key=\"item.time + item.title\">\n                <td>{{ item.time }}</td>\n                <td>{{ item.title }}</td>\n                <td>{{ item.location }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </section> -->\n\n    <!-- 2025年11月日曆區塊 -->\n    <section id=\"calendar\" class=\"calendar-section\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">活動日曆</h2>\n        <Calendar />\n      </div>\n    </section>\n\n  \n    <!-- 關於區塊 -->\n    <section id=\"about\" class=\"about\">\n      <div class=\"container\">\n        <h2 class=\"section-title\" style=\"margin:0;\">關於我們</h2>\n        <div class=\"about-content\">\n          <div class=\"about-text\">\n            <p>FVL DOME在文化部的支持下，於2023年開啟硬體升級計畫，尺寸突破過往尺度，打造直徑15公尺360度穹頂劇場。\n              C-LAB穹頂劇場透過軟硬體歷經數次迭代開發，克服球形曲面投影的校正、融接、對位、播放控制與影像前製等多重技術之複雜度，沉浸影像投影系統總運算可達 8K*8K超高解析度；\n              內部以雙層結構隱藏影音設備，客製新型態透聲投影膜片、建構出更完善的25.4聲道環繞聲場環境。</p>\n          </div>\n          <div class=\"about-stats\">\n            <div class=\"stat-item\">\n              <h3>DOME照片</h3>\n              <!-- <p>完成專案</p> -->\n            </div>\n            <!-- <div class=\"stat-item\">\n              <h3>30+</h3>\n              <p>滿意客戶</p>\n            </div>\n            <div class=\"stat-item\">\n              <h3>5+</h3>\n              <p>年經驗</p>\n            </div> -->\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 服務區塊 -->\n    <section id=\"services\" class=\"services\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">關於DOME</h2>\n        <div class=\"services-grid\">\n          <div class=\"service-card\">\n            <!-- <div class=\"service-icon\">💻</div> -->\n            <h3>DOME1.0</h3>\n            <p>瞭解關於1.0的搭建過程和內容</p>\n          </div>\n          <div class=\"service-card\">\n            <!-- <div class=\"service-icon\">🎨</div> -->\n            <h3>DOME2.0</h3>\n            <p>瞭解關於1.0的搭建過程和內容</p>\n          </div>\n          <div class=\"service-card\">\n            <!-- <div class=\"service-icon\">📱</div> -->\n            <h3>播映作品</h3>\n            <p>DOME的播映作品</p>\n          </div>\n          <div class=\"service-card\">\n            <!-- <div class=\"service-icon\">☁️</div> -->\n            <h3>徵件作品</h3>\n            <p>過往作品</p>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 作品集區塊 -->\n    <section id=\"portfolio\" class=\"portfolio\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">作品集</h2>\n        <div class=\"portfolio-grid\">\n          <div class=\"portfolio-item\">\n            <div class=\"portfolio-image\">\n              <img src=\"https://via.placeholder.com/300x200/4CAF50/ffffff?text=專案1\" alt=\"專案1\">\n            </div>\n            <div class=\"portfolio-overlay\">\n              <h3>電商網站</h3>\n              <p>現代化的電子商務平台</p>\n            </div>\n          </div>\n          <div class=\"portfolio-item\">\n            <div class=\"portfolio-image\">\n              <img src=\"https://via.placeholder.com/300x200/2196F3/ffffff?text=專案2\" alt=\"專案2\">\n            </div>\n            <div class=\"portfolio-overlay\">\n              <h3>企業官網</h3>\n              <p>專業的企業形象網站</p>\n            </div>\n          </div>\n          <div class=\"portfolio-item\">\n            <div class=\"portfolio-image\">\n              <img src=\"https://via.placeholder.com/300x200/FF9800/ffffff?text=專案3\" alt=\"專案3\">\n            </div>\n            <div class=\"portfolio-overlay\">\n              <h3>行動應用</h3>\n              <p>跨平台行動應用程式</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n      <!-- 製作團隊區塊 -->\n      <section id=\"team\" class=\"team\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">製作團隊</h2>\n        <div class=\"team-list\">\n          <div class=\"team-item\"><span class=\"team-role\">計畫主持人</span>：蔡奇宏</div>\n          <div class=\"team-item\"><span class=\"team-role\">節目統籌</span>：廖苑喻</div>\n          <div class=\"team-item\"><span class=\"team-role\">技術統籌</span>：蔡奇宏</div>\n          <div class=\"team-item\"><span class=\"team-role\">技術執行</span>：王竣恆</div>\n          <div class=\"team-item\"><span class=\"team-role\">技術協力</span>：林瀚寬</div>\n          <div class=\"team-item\"><span class=\"team-role\">前期建築概念設計</span>：陽明交通大學建築研究所 JHStudio</div>\n          <div class=\"team-item\"><span class=\"team-role\">建築協力</span>：achy_made</div>\n          <div class=\"team-item\"><span class=\"team-role\">聲場設計</span>：C-LAB 臺灣聲響實驗室</div>\n          <div class=\"team-item\"><span class=\"team-role\">音響系統統籌</span>：黑米創意工作室</div>\n          <div class=\"team-item\"><span class=\"team-role\">開場影像</span>：吳克軍</div>\n          <div class=\"team-item\"><span class=\"team-role\">進場影像</span>：榊原澄人</div>\n          <div class=\"team-item\"><span class=\"team-role\">主視覺設計</span>：林羅伯</div>\n          <div class=\"team-item\"><span class=\"team-role\">網站設計</span>：林瀚寬</div>\n          <div class=\"team-item\"><span class=\"team-role\">靜態攝影</span>：林軒朗、ANPIS FOTO 王世邦</div>\n          <div class=\"team-item\"><span class=\"team-role\">動態攝影</span>：散步映畫有限公司</div>\n          <div class=\"team-item\"><span class=\"team-role\">宣傳片剪接</span>：黃嘉文</div>\n          <div class=\"team-item\"><span class=\"team-role\">翻譯</span>：高慧倩</div>\n        </div>\n      </div>\n    </section>\n\n\n    <!-- 聯絡我們區塊 -->\n    <section id=\"contact\" class=\"contact\">\n      <div class=\"container\">\n        <h2 class=\"section-title\">聯絡我們</h2>\n        <div class=\"contact-content\">\n          <div class=\"contact-info\">\n            <h3>讓我們開始合作</h3>\n            <p>如果您有任何問題或想要討論專案，請隨時與我們聯絡。</p>\n            <div class=\"contact-details\">\n              <div class=\"contact-item\">\n                <span class=\"contact-icon\">📧</span>\n                <span>info@fvl2025.com</span>\n              </div>\n              <div class=\"contact-item\">\n                <span class=\"contact-icon\">📞</span>\n                <span>+886 912 345 678</span>\n              </div>\n              <div class=\"contact-item\">\n                <span class=\"contact-icon\">📍</span>\n                <span>台北市信義區</span>\n              </div>\n            </div>\n          </div>\n          <form class=\"contact-form\" @submit.prevent=\"submitForm\">\n            <div class=\"form-group\">\n              <input type=\"text\" v-model=\"form.name\" placeholder=\"您的姓名\" required>\n            </div>\n            <div class=\"form-group\">\n              <input type=\"email\" v-model=\"form.email\" placeholder=\"您的信箱\" required>\n            </div>\n            <div class=\"form-group\">\n              <textarea v-model=\"form.message\" placeholder=\"您的訊息\" rows=\"5\" required></textarea>\n            </div>\n            <button type=\"submit\" class=\"submit-button\">發送訊息</button>\n          </form>\n        </div>\n      </div>\n    </section>\n\n    <!-- 頁腳 -->\n    <FooterBar />\n\n    <!-- LiveDrawer 結構 -->\n    <div class=\"live-drawer\" :class=\"{ open: showLiveDrawer }\">\n      <button class=\"drawer-close\" @click=\"showLiveDrawer = false\">×</button>\n      <div class=\"drawer-content\">\n        <h1>LIVE 演出</h1>\n        <p>這裡可以放置 LIVE 演出的詳細內容、列表、影片、報名連結等。</p>\n        <img src=\"assets/2025banner.jpg\" style=\"width: 100%;height:100%;z-index:999;\">\n      </div>\n    </div>\n       <!-- Drawer 結構 -->\n       <div class=\"drawer\" :class=\"{ open: showDrawer }\">\n      <button class=\"drawer-close\" @click=\"showDrawer = false\">×</button>\n      <div class=\"drawer-content2\">\n        <h1>播映作品</h1>\n        <p></p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// import NavBar from './components/NavBar.vue';\nimport FooterBar from './components/FooterBar.vue';\nimport Calendar from './components/Calendar.vue';\nimport axios from 'axios';\nimport { gsap } from 'gsap';\nimport { SplitText } from 'gsap/SplitText';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\n\n\n// 註冊 SplitText 和 ScrollTrigger 插件\ngsap.registerPlugin(SplitText, ScrollTrigger);\n\nexport default {\n  name: 'App',\n  components: { FooterBar, Calendar },\n  data() {\n    return {\n      form: {\n        name: '',\n        email: '',\n        message: ''\n      },\n      schedule: [\n        { time: '10:00 - 11:00', title: '開幕式', location: '主舞台' },\n        { time: '11:15 - 12:00', title: '主題演講：未來科技', location: '會議廳A' },\n        { time: '13:00 - 14:30', title: '工作坊：互動設計', location: '教室B' },\n        { time: '15:00 - 16:00', title: '圓桌論壇', location: '會議廳A' },\n        { time: '16:30 - 17:00', title: '閉幕式', location: '主舞台' }\n      ],\n      showLiveDrawer: false,\n      showDrawer:false,\n    }\n  },\n  mounted() {\n    this.initTextAnimation();\n    this.fetchDatabaseData();\n    this.pinBanner();\n  },\n  methods: {\n    // 取得資料庫資料並在 console 顯示\n    async fetchDatabaseData() {\n      try {\n        const response = await axios.get('http://localhost:3001/api/data');\n        console.log(response.data);\n        // console.log('📋 資料筆數:', response.data.length);\n        // console.log('🔍 詳細資料:', JSON.stringify(response.data, null, 2));\n        return response.data;\n      } catch (error) {\n        console.error('❌ 取得資料庫資料失敗:', error);\n        return [];\n      }\n    },\n    scrollToSection(sectionId) {\n      const element = document.getElementById(sectionId);\n      if (element) {\n        element.scrollIntoView({ behavior: 'smooth' });\n      }\n    },\n    submitForm() {\n      alert('感謝您的訊息！我們會盡快回覆您。');\n      this.form = { name: '', email: '', message: '' };\n    },\n    initTextAnimation() {\n      // 分割 FUTURE VISION LAB 2025 標題\n      const splitTitle = document.querySelector('.split')\n      if (splitTitle) {\n        const splitText = new SplitText(splitTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        })\n        \n        // 設置初始狀態\n        gsap.set(splitText.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        })\n        \n        // 動畫效果\n        gsap.to(splitText.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        })\n      }\n      \n      // 分割首頁標題文字\n      const heroTitle = document.querySelector('.hero-title')\n      if (heroTitle) {\n        const splitTitle = new SplitText(heroTitle, {\n          type: \"chars, words\",\n          charsClass: \"char\",\n          wordsClass: \"word\"\n        })\n        \n        // 設置初始狀態\n        gsap.set(splitTitle.chars, {\n          opacity: 0,\n          y: 50,\n          rotationX: -90\n        })\n        \n        // 動畫效果\n        gsap.to(splitTitle.chars, {\n          opacity: 1,\n          y: 0,\n          rotationX: 0,\n          duration: 0.8,\n          stagger: 0.05,\n          ease: \"back.out(1.7)\"\n        })\n      }\n      \n      // 分割副標題\n      const heroSubtitle = document.querySelector('.hero-subtitle')\n      if (heroSubtitle) {\n        const splitSubtitle = new SplitText(heroSubtitle, {\n          type: \"words\",\n          wordsClass: \"word\"\n        })\n        \n        gsap.set(splitSubtitle.words, {\n          opacity: 0,\n          y: 30\n        })\n        \n        gsap.to(splitSubtitle.words, {\n          opacity: 1,\n          y: 0,\n          duration: 0.6,\n          stagger: 0.1,\n          delay: 0.5,\n          ease: \"power2.out\"\n        })\n      }\n      \n      // ScrollTrigger 動畫 - 節目時間表\n      gsap.fromTo('#schedule .section-title, #schedule .schedule-table', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#schedule\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n            toggleActions: \"play none none reverse\"\n          }\n        }\n      )\n      \n      // ScrollTrigger 動畫 - 關於DOME\n      gsap.fromTo('#services .section-title, #services .service-card', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#services\",\n            start: \"top 80%\",\n            end: \"bottom 20%\",\n            toggleActions: \"play none none reverse\"\n          }\n        }\n      )\n      \n      // ScrollTrigger 動畫 - 作品集\n      gsap.fromTo('#portfolio .section-title, #portfolio .portfolio-item', \n        {\n          opacity: 0,\n          y: 50\n        },\n        {\n          opacity: 1,\n          y: 0,\n          duration: 0.8,\n          stagger: 0.2,\n          ease: \"power2.out\",\n          scrollTrigger: {\n            trigger: \"#portfolio\",\n            start: \"top 80%\",\n            end: \"bottom